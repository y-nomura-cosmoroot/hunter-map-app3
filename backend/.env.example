# Application Settings
APP_NAME="PDF Red Box KML Converter"
DEBUG=false

# File Upload Settings
UPLOAD_DIR=temp
MAX_FILE_SIZE=52428800
FILE_TTL=3600

# Logging Settings
LOG_LEVEL=INFO

# CORS Settings (comma-separated list)
CORS_ORIGINS=http://localhost:3000,http://localhost:5173

# Image Processing Settings
MAX_IMAGE_WIDTH=4000
MAX_IMAGE_HEIGHT=4000
PDF_DPI=300

# Red Box Detection Settings
# Minimum area in pixels (default: 500)
# Increase to filter out smaller regions (e.g., 1000, 2000, 5000)
MIN_BOX_AREA=500

# Minimum perimeter in pixels (default: 50)
# Increase to filter out smaller regions (e.g., 100, 150, 200)
MIN_BOX_PERIMETER=50

# Polygon approximation epsilon (default: 0.01)
# Smaller values = more accurate polygon shape
APPROX_EPSILON=0.01

# Polygon vertices constraints
MIN_BOX_VERTICES=3
MAX_BOX_VERTICES=50
